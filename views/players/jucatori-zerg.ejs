<% include ../partials/sidebar %>
<% }); %>

<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">ZERG</h1>
        </div>
        <!-- /.col-lg-12 -->
    </div>
    <!-- /.row -->
    <div class="row">
        <div class="col-lg-12">
            <!-- /.panel -->
            <div class="panel panel-default">
                <div class="panel-heading">
                    <i class="fa fa-clock-o fa-fw"></i> Lista jucatori zerg
                </div>
                <!-- /.panel-heading -->
                <div class="panel-body">
                    <table class="table table-hover">
                        <tr>
                            <th>Nickname jucator</th>
                            <th>Battle.net ID</th>
                            <th>Liga</th>
                            <th>Website</th>
                        </tr>

                        <% zergPlayers.forEach(function(player){ %>
                        <tr>
                            <td><%= player.local.nickname %></td>
                            <td><%= player.local.battlenetid %></td>
                            <td><%= player.local.league %></td>
                            <td>
                                <%= player.local.website == "" ? 'Nu exista' : player.local.website %>
                            </td>
                        </tr>
                        <% }); %>

                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
</div>


<% include ../partials/footer %>